! Font settings
URxvt.font: xft:Inconsolata for Powerline:antialias=true:Medium:pixelsize=14
xterm*faceName: xft:Inconsolata for Powerline:antialias=true:Medium:pixelsize=14
URxvt.letterSpace: 1

! Scrollbar
URxvt.scrollBar: false
URxvt.scrollBar_right: true
URxvt.scrollstyle: plain
URxvt.scrollColor: #2d2d2d
! Transparency
! URxvt.transparent: true
URxvt.shading: 9

! Clickable links
URxvt.perl-ext-common: default,matcher,selection-to-clipboard,keyboard-select
URxvt.url-launcher: /usr/bin/google-chrome-stable
URxvt.matcher.button: 1
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-u:     perl:url-select:select_next
URxvt.clipboard.autocopy: true


! Ctrl+Shift remove
URxvt.iso14755: false
URxvt.iso14755_52: false

! Colours
URxvt.depth: 24

*background: #282828
*foreground: #ebdbb2
! Black + DarkGrey
*color0: #282828
*color8: #928374
! DarkRed + Red
*color1: #cc241d
*color9: #fb4934
! DarkGreen + Green
*color2: #98971a
*color10: #b8bb26
! DarkYellow + Yellow
